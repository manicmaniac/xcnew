; See https://reverse.put.as/wp-content/uploads/2011/09/Apple-Sandbox-Guide-v1.0.pdf for syntax.
; To see log of sandbox, run `log stream --level error --predicate 'process = "kernel" AND sender = "Sandbox"'`

(version 1)

(deny default)
(allow process-exec*
    (regex #"XCBBuildService")
    (regex #"xcnew$"))
(allow process-fork)
(allow ipc-posix-shm*)
(allow iokit-open-user-client)
(allow mach-lookup)
(allow file-ioctl
    (literal "/dev/dtracehelper"))
(allow file-read*)
(allow file-write*
    (subpath "/private/var/folders")
    (literal "/dev/dtracehelper")
    (literal "/dev/null"))
(allow job-creation)
(allow sysctl*)
(allow system-fsctl)
(allow user-preference-read)
(allow user-preference-write)
